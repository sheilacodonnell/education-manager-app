  <table class="table table-striped">
    <thead>
      <tr class="<%= custom_style %>"%>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">Grade</th>
      </tr>
    </thead>
    <tbody>
      <% students.each do |student|%>
        <tr>
          <td> <%= student.first_name %></td>
          <td> <%= student.last_name %></td>
          <td> <%= student.grade %></td>
        </tr>
      <% end %>
    </tbody>
  </table>